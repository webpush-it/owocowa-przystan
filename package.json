{
  "name": "owocowa-przystan-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "start": "concurrently -n \"studio,web\" -c \"cyan,magenta\" \"npm run dev -w studio\" \"vercel dev\"",
    "build": "npm run build --workspaces",
    "deploy": "vercel deploy --prod",
    "deploy:web": "vercel deploy --prod",
    "deploy:studio": "npm run deploy -w studio"
  },
  "dependencies": {
    "@sanity/astro": "^3.2.11",
    "@sanity/client": "^7.14.1",
    "astro": "^5.16.15"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "vercel": "^50.9.6"
  }
}
